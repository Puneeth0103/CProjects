#include<stdio.h>
#include<string.h>

struct employee
{
	int empid;
	char empname[30];
	int age;
	double phno;
	int salary;
};

void display();

struct employee E[20];
int i;

void display()
{
	printf("ID		Name		  Age Contact No. Salary\n");
	for(i=0;i<20;i++)
	{
		printf("%d %s %d %d %d \n",E[i].empid,E[i].empname,E[i].age,E[i].phno,E[i].salary);
	}
}

main()
{
	
	for(i=0;i<20;i++)
	{
		printf("Employee no.: %d\n",i+1);
		printf("Enter employee ID:\n");
		scanf("%d",&E[i].empid);
		
		printf("Enter employee name:\n");
		scanf("%s",E[i].empname);
		while(strlen(E[i].empname)<30)
			strcat(E[i].empname," ");
		
		printf("Enter employee age:\n");
		scanf("%d",&E[i].age);
		
		printf("Enter employee contact no.:\n");
		scanf("%d",&E[i].phno);
		
		printf("Enter employee salary:\n");
		scanf("%d",&E[i].salary);
	}
	
	display();
	
}
